.panel.panel-primary
  .panel-body
    .text-center
      = link_to "ICU Junior Ratings", "http://ratings.icu.ie/icu_ratings/juniors", target: "ratings"

.panel.panel-primary.clubs
  .panel-heading
    .text-center= t('junior.clubs')
  .panel-body
    %ul
      - @junior_clubs.each do |club|
        %li= link_to club.name, club
