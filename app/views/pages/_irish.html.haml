.panel.panel-primary
  .panel-heading
    =link_to 'Upcoming Events', events_path
  - events.each do |event|
    .panel.panel-default
      .panel-heading
        = link_to event.name, event
      .panel-body
        = event.dates
        %br
        = ((event.note || '').strip.split("\n").first || '').strip
